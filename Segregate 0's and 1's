#include <stdio.h>
int main()
{
   // Try out your code here
  long int n,arr[1000],count=0;
  scanf("%ld",&n);
  for(int i=0;i<n;i++)
  {
    scanf("%ld", &arr[i]);
  }
  for(int i=0;i<n;i++)
  {
    if(arr[i]==0)
    {
    	printf("0");
    }else
    {
    	count++;
    }
  }
  for(int i=0;i<count;i++)
  {
  	printf("1");
  }
   
   return 0;
}
